{
  "project_name": "myproject",
  "core": {
    "location": "root",
    "path": "core"
  },
  "directories": [
    {
      "name": "hr",
      "parent": ""
    }
  ],
  "apps": [
    {
      "name": "users",
      "directory": "hr"
    }
  ],
  "services": [
    {
      "name": "redis",
      "options": {
        "use_for_cache": true,
        "use_for_sessions": true
      }
    },
    {
      "name": "celery",
      "options": {
        "broker": "redis",
        "use_flower": true
      }
    }
  ],
  "env": "dev"
}